{
  "name": "bro-start",
  "version": "2.0.0",
  "description": "Blazing fast webpack setup for static websites",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development node scripts/serve.js",
    "build": "cross-env NODE_ENV=production node scripts/build.js",
    "deploy": "npm run build && ghp build",
    "deps": "dependency-check package.json --entry src/app.js",
    "lint": "snazzy",
    "test": "npm run deps && npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brocessing/bro-start"
  },
  "author": "Brocessing",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brocessing/bro-start/issues"
  },
  "homepage": "https://github.com/brocessing/bro-start#readme",
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "browser-sync": "^2.18.8",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^3.2.4",
    "css-loader": "^0.27.3",
    "dependency-check": "^2.8.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "ghp": "^1.1.4",
    "handlebars": "^4.0.6",
    "kool-shell": "^1.4.3",
    "php-server-middleware": "^0.4.0",
    "postcss-loader": "^1.3.3",
    "progress-bar-webpack-plugin": "^1.9.3",
    "snazzy": "^6.0.0",
    "strip-loader": "^0.1.2",
    "style-loader": "^0.16.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "webpack": "^2.3.2",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.17.1",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {}
}
